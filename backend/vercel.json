{
    "version": 2,
    "builds": [
      {
        "src": "/src/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/auth/(.*)",
        "dest": "/src/index.js"
      },
      {
        "src": "/api/projects/(.*)",
        "dest": "/src/index.js"
      },
      {
        "src": "/api/messages/(.*)",
        "dest": "/src/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }
  